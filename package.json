    "lodash": "^4.17.4",
    "popper.js": "^1.12",
    "vue": "^2.5.7",
    "babel-plugin-transform-runtime": "^6.1.18",
    "babel-preset-es2015": "^6.1.18",
    "babel-runtime": "^6.2.0",
    "gulp": "^3.9.0",
    "vue-hot-reload-api": "^1.2.1",
    "vueify": "^6.0.0",
    "vueify-insert-css": "^1.0.0",
    "laravel-elixir-vueify": "^1.0.0"
